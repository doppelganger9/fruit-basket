    "prettier:write": "prettier src/**/*.js --write",
    "prettier:check": "prettier src/**/*.js --check",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [],
  "author": "David LACOURT <david.lacourt@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "3.0.2",
    "jest": "23.6.0",
    "prettier": "1.16.4"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
